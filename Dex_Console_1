using System;

namespace DataTypeApp
{
    class DataTypeProgram
    {
        static void Main(string[] args)
        {
            int _dexie;
            _dexie = 150;

            double Erika = 30.05;

            const int Abbie = 1500;

            char charot = 'J';

            bool Apple = true;

            Console.WriteLine(_dexie + " " + Erika + " " + Abbie + " " + charot + " " + Apple);
        }
    }
}
